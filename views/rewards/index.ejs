<h2>Search Rewards</h2>
<form action="/rewards" method="GET">
    <div>
        <label>Name</label>
        <input type="text" name="title" value="<%= searchOptions.name %>">
    </div>

    <button type="submit">Search</button>
</form>
<div>
    <% rewards.forEach(reward => { %>
        <a href="/rewards/<%= reward.id %>">
            <img height="150" width="100" src="<%= reward.coverImagePath %>" alt="">
        </a>
    <% }) %> 
</div>

